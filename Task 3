#include <QCoreApplication>
#include <QDebug>
#include <QTextStream>

int main(int argc, char *argv[])
{
    QCoreApplication a(argc, argv);
    QTextStream usinp(stdin);


    qDebug() << "Enter the number: ";
    int input = usinp.readLine().trimmed().toInt();

    if(input > 0){
        qDebug() << "Your number is positive";
    }else if(input < 0){
        qDebug() << "Your number is negative";
    }else{
        qDebug() << "Your number is 0";
    }

    char grade = 0;

    switch (input) {
        case 90 ... 100:
            grade = 'A';
            break;
        case 80 ... 89:
            grade = 'B';
            break;
        case 70 ... 79:
            grade = 'C';
            break;
        case 60 ... 69:
            grade = 'D';
            break;
        case 50 ... 59:
            grade = 'E';
            break;
        default:
            grade = 0;
            qDebug() << "We do not have such grade";
    }

    if(grade){
        qDebug() << "Your grade is: " << grade;
    }


    for(int i = 1; i <= 10; ++i){
        qDebug() << i;
    }

    return a.exec();
}
