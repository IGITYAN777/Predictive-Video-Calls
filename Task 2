#include <QCoreApplication>
#include <QDebug>

int main(int argc, char *argv[])
{
    QCoreApplication a(argc, argv);

    int number = 7;
    float flt = 7.77f;
    double dbl = 77.7;
    char ch = 'a';
    bool bl = true;

    qDebug() << number;
    qDebug() << flt;
    qDebug() << dbl;
    qDebug() << ch;
    qDebug() << bl;

    return a.exec();
}
